<h1>Eat-Da-Burger!</h1>

<h2>Uneaten Burgers</h2>

<ul>
  {{#each burgers}}
    {{#unless this.devoured}}
      <li>
        {{this.burger_name}}
      </li>
    {{/unless}}
  {{/each}}
</ul>

<h2>Eaten Burgers</h2>
  
 <ul>
  {{#each burgers}}
    {{#if this.devoured}}
      <li>
        {{this.burger_name}}
      </li>
    {{/if}}
  {{/each}}
</ul>


